{
  "name": "email-attendant",
  "version": "1.0.0",
  "description": "Sistema de categorização e resposta automática de emails Gmail",
  "main": "appserver/dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd appserver && npm run dev",
    "client:dev": "cd appclient && npm run dev",
    "server": "cd appserver && npm run dev",
    "client": "cd appclient && npm run dev",
    "install:all": "npm install && cd appclient && npm install --legacy-peer-deps && cd ../appserver && npm install",
    "install:windows": "install-windows.bat",
    "install:linux": "./install.sh",
    "build": "npm run build:server && npm run build:client",
    "build:server": "cd appserver && npm run build",
    "build:client": "cd appclient && npm run build",
    "start": "cd appserver && npm start",
    "typecheck": "cd appserver && npm run typecheck && cd ../appclient && npm run typecheck",
    "test": "cd appclient && npm test",
    "setup": "cd appserver && npm run setup"
  },
  "keywords": [
    "gmail",
    "email",
    "categorization",
    "automation",
    "typescript",
    "react",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "googleapis": "^129.0.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "tsx": "^4.20.4",
    "typescript": "^5.3.2"
  }
}
